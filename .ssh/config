Include config.d/*

Host *.ho
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    HostName %hmelab

Host *.homelab
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host 192.168.1.*
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    Ciphers aes128-gcm@openssh.com
    Compression no
